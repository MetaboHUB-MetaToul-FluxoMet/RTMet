#!/bin/bash
# Inspired by
# https://github.com/cylc/cylc-flow/blob/master/etc/bin/run-validate-tutorials


set -eu
cd "$(dirname "$0")/../../"

for WF in bioreactor-workflow; do
    echo "# Running lint/validate tests for: $WF"
    cd "cylc-src/$DIR"
    set -x
    cylc lint .
    cylc validate .
done