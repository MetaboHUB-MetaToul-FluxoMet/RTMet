{
    "tables": {
        "features": {
            "datetime_column": "datetime",
            "fields": {
                "cycle": "int",
                "mz": "float",
                "intensity": "float",
                "purity": "float",
                "centrality": "float"
            },
            "tags": [
                "instrument_id",
                "polarity",
                "bin",
                "merged_peak",
                "annotated"
            ]
        },
        "concentrations": {
            "datetime_column": "datetime",
            "fields": {
                "cycle": "int",
                "concentration": "float",
                "feature_mz": "float",
                "delta_ppm": "float",
                "intensity": "float",
                "relative_c": "float",
                "absolute_c": "float"
            },
            "tags": [
                "instrument_id",
                "isobaric_id",
                "std_id",
                "feature_polarity"
